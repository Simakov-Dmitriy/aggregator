                    $('.top').css('background', 'url("../images/city-2-top.png") top no-repeat');
                    $('.section-2').css('background', 'url("../images/city-2-middle.png") top no-repeat');
                    $('.section-3').css('background', 'url("../images/city-2-bottom.png") top no-repeat');
                } else {
                    $('.top').css('background', 'url("../images/city-2-top.webp") top no-repeat');
                    $('.section-2').css('background', 'url("../images/city-2-middle.webp") top no-repeat');
                    $('.section-3').css('background', 'url("../images/city-2-bottom.webp") top no-repeat');
                }
                $('.top').css('background-size', 'cover');
                $('.section-2').css('background-size', 'cover');
                $('.section-3').css('background-size', 'cover');
                $('.top,.section-4,.section-2,.section-3').addClass('city-2-bg');
                $(".top-header-cart-counter").text(cartCounter2);
                $('.top-header-cart,.red-btn').attr('href', '/Home/Basket?St-Augustine');
            }
            $('.red-btn,.top-header-cart').click(function (e) {
                if (!$(this).hasClass('target-link')) {
                    var cardCounter = JSON.parse(localStorage.getItem("cartCounter2"));
                    if (cardCounter < 1) {
                        e.preventDefault();
                    }
                }
            });
            $(window).on("resize", function () {
                if ($(window).width() > 1065) {
                    initialPage();
                } else {
                    $('img').attr('data-src', '');
                }
        }); 
        </script> 
